{{#if isLoaded}}
    <div>{{#linkTo "mobileApps"}}Push Applications{{/linkTo}} > {{#linkTo "variants" this}}Variants{{/linkTo}} > {{#linkTo "variant" this}}Variant{{/linkTo}}</div>
    <div class="header">
        <h2>{{ name }}</h2>
        <span>{{ description }}</span>
    </div>
    <div class="sub-header">
        <h3>Authentication Info</h3>
